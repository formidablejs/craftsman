{
  "name": "@formidablejs/craftsman",
  "version": "1.0.0",
  "author": "Donald Pakkies <donaldpakkies@gmail.com>",
  "bin": {
    "craftsman": "./bin/run"
  },
  "bugs": "https://github.com/formidablejs/craftsman/issues",
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/src"
  ],
  "homepage": "https://github.com/formidablejs/craftsman",
  "keywords": [
    "formidable",
    "formidablejs",
    "craftsman",
    "development cli"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "repository": "formidablejs/craftsman",
  "scripts": {
    "build": "npm run imba:build && npm run imba:build:temp && npm run ts:build",
    "imba:build": "imbac src --output=lib --platform=node --format=cjs --clean",
    "imba:build:temp": "imbac src --output=.temp --platform=node --clean",
    "imba:watch": "imbac -w src --output=lib --platform=node --clean",
    "ts:build": "tsc",
    "ts:watch": "tsc -w",
    "test": "echo NO TESTS"
  },
  "dependencies": {
    "@formidablejs/console": "^0.0.1"
  },
  "devDependencies": {
    "imba": "^2.0.0-alpha.201"
  }
}